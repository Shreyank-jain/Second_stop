int atoi(string str) {
        int flag=1,i=0,ans=0;
        if(str[i]=='-')
        {
            flag=-1;i++;
        }
        for(;str[i]!='\0';i++)
        {
            if(str[i]>='0' && str[i]<='9')
            {
                ans=ans*10 + str[i]-'0';
            }
            else 
             return -1;
            
        }
        return ans*flag;
    }
