bool canPair(vector<int> nums, int k) {
        int n=nums.size(),sum=0;
        if(n==1) return 0;
        unordered_map<int,int> mp;
        mp[0]=0;mp[k/2]=0;
        
        for(int i=0;i<n;i++)
            mp[nums[i]%k]++;
            
        if(mp[0]%2==1 || k%2==0 && (mp[k/2]%2)==1 ) return 0;
        
        for(int i=1;i<=k/2;i++)
            if(mp[i] != mp[k-i])
                return 0;
                
        return 1;
    }
