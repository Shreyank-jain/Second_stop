int getNthFromLast(Node *head, int n)
{
        if(head==NULL) return -1;
        struct Node *prev = NULL;
        struct Node *current = head;
        struct Node *next ;
        int count=0;
        while(current !=NULL)
        {
            next=current->next;
            current->next = prev;
            prev=current;
            current = next;
            count++;
        }
        if(n>count) return -1;
        for(int i=1;i<n;i++)
        {
            prev=prev->next;
        }
        return prev->data;
        
}
